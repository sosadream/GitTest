    total = 0
    for app in dataset:
        total += 1
        value = app[index]
        if value in frequency:
            frequency[value] += 1
        else:
            frequency[value] = 1
    
    freq_percentage = {}
    for i in frequency:
        percentage = (frequency[i] * 100) / total
        freq_percentage[i] = percentage
    
    print(frequency,'\n')
        

